{
  "name": "@phosphor-icons/core",
  "version": "1.4.1",
  "description": "Icon catalog and assets for Phosphor",
  "license": "MIT",
  "repository": "https://github.com/phosphor-icons/phosphor-core.git",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "author": {
    "name": "Tobias Fried",
    "email": "friedtm@gmail.com",
    "url": "https://tobiasfried.com"
  },
  "keywords": [
    "phosphor",
    "icons",
    "svg",
    "design",
    "interface",
    "UI",
    "UX"
  ],
  "files": [
    "./dist",
    "./assets"
  ],
  "scripts": {
    "fetch": "node ./bin/fetch.js && yarn format",
    "format": "prettier --write ./src/**",
    "build": "vite build && tsc --emitDeclarationOnly"
  },
  "devDependencies": {
    "@types/node": "^18",
    "axios": "^1.2.2",
    "chalk": "^4",
    "commander": "^9.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  }
}